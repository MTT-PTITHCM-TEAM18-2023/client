{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "src/*": ["./src/*"],
      "@components/*": ["src/components/*"],
      "@utils/*": ["src/utils/*"],
      "@services/*": ["src/services/*"],
      "@style/*": ["src/style/*"],
      "@store/*": ["src/store/*"],
      "@apis/*": ["src/services/*"],
      "@common/*": ["src/common/*"],
      "@styles/*": ["src/styles/*"],
      "@layouts/*": ["src/layouts/*"],
      "@redux/*": ["src/redux/*"],
      "@routers/*": ["src/routers/*"],
      "@assets/*": ["src/assets/*"]
    }
  },
  "exclude": ["node_modules", "build"]
}